ListMenu "MainMenu"
{
  StaticPatchCentered 160, 4, "M_SQUARE"

  Font "SmallFont", "gold"
  StaticTextCentered 160, 52, "$SQMENU_SUBTITLE"

  Position 108, 64

  Font "BigFont", "White"
  TextItem "$SQMENU_STARTGAME", "s", "PlayerclassMenu"
  TextItem "$SQMENU_READTHIS" , "r", "ReadThisMenu"
  TextItem "$SQMENU_LOAD"     , "l", "LoadGameMenu"
  TextItem "$SQMENU_SAVE"     , "s", "SaveGameMenu"
  TextItem "$SQMENU_OPTIONS"  , "o", "MOOption" // Don't monkey with ZDoom's own Options menu.
  TextItem "$SQMENU_ENDGAME"  , "e", "EndGameMenu"
  TextItem "$SQMENU_QUIT"     , "q", "QuitMenu"
}

